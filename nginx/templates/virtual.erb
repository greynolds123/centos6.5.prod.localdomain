#
# A virtual host using mix of IP-, name-, and port-based configuration
#

server {
    listen       8280;
    listen       centos6.5.localdomain:8280;
    server_name  centos6.5.localdomain  alias  centos6.5.alias;

    location / {
        root   html;
        index  index.html index.htm;
    }
}

