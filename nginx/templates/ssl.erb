#
# HTTPS server configuration
#

server {
    listen       443;
    server_name  centos6.5.localdomain;

    ssl                  on;
    ssl_certificate      /etc/pki/tls/certs/centos6.5.localdomain.pem;
    ssl_certificate_key  /etc/pki/tls/private/centos6.5.localdomain.key;

    ssl_session_timeout  5m;

    ssl_protocols  SSLv2 SSLv3 TLSv1;
    ssl_ciphers  ALL:!ADH:!EXPORT56:RC4+RSA:+HIGH:+MEDIUM:+LOW:+SSLv2:+EXP;
    ssl_prefer_server_ciphers   on;

    location / {
        root   html;
        index  index.html index.htm;
    }
}

